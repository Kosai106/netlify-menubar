{
  "name": "netlify-menubar",
  "version": "1.0.0",
  "description": "A menubar app to keep track of Netlify builds",
  "author": "stefan judis <stefanjudis@gmail.com>",
  "main": "index.js",
  "scripts": {
    "dev": "tsc && electron index.js",
    "postinstall": "electron-builder install-app-deps",
    "test": "tslint src/*",
    "pack": "electron-builder --dir -m",
    "dist": "electron-builder -m",
    "release": "electron-builder -p onTagOrDraft"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stefanjudis/netlify-menubar-status.git"
  },
  "build": {
    "appId": "netlify.menubar.electron.app",
    "productName": "Netlify Menubar",
    "copyright": "Copyright Â© 2019 Stefan Judis",
    "directories": {
      "output": "built"
    }
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stefanjudis/netlify-menubar-status/issues"
  },
  "homepage": "https://github.com/stefanjudis/netlify-menubar-status#readme",
  "dependencies": {
    "date-fns": "^1.30.1",
    "electron-settings": "^3.2.0",
    "node-fetch": "^2.3.0"
  },
  "devDependencies": {
    "@types/electron-settings": "^3.1.1",
    "@types/node-fetch": "^2.1.4",
    "electron-builder": "^20.38.2",
    "husky": "^1.2.0",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  }
}
